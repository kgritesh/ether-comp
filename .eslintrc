{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "react/forbid-prop-types": 0,
    "jsx-a11y/anchor-has-content": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "arrow-parens": 0
  }
}
