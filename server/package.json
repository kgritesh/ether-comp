    "debug": "~2.6.3",
    "enumify": "^1.0.4",
    "express": "~4.15.2",
    "global": "^4.3.2",
    "googleapis": "^20.1.0",
    "helmet": "^3.8.0",
    "morgan": "~1.8.1",
    "raven": "^2.1.1",
    "serve-favicon": "~2.4.2",
    "thinky": "^2.3.8"
  },
